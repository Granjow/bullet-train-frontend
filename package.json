{
  "name": "bullet-train-frontend",
  "version": "1.3.0",
  "description": "The frontend application for Bullet Train",
  "main": "index.js",
  "scripts": {
    "kill": "kill -9 $(lsof -ti tcp:4444)",
    "test": "npm run kill & cross-env E2E=true nightwatch ./tests/index.test.js",
    "env": "node ./bin/env.js",
    "start": "cross-env NODE_ENV=production node ./server",
    "dev": "cross-env npm run env && node_modules/.bin/nodemon --watch server --ignore server/config/config.json --watch webpack --exec node ./server",
    "bundle": "node node_modules/.bin/webpack -p --progress --config ./webpack/webpack.config.prod.js",
    "bundlestaging": "node node_modules/.bin/webpack -p --progress --config ./webpack/webpack.config.staging.js",
    "bundledev": "node node_modules/.bin/webpack -p --progress --config ./webpack/webpack.config.dev.js"
  },
  "engines": {
    "node": "8.x",
    "npm": "5.6.0"
  },
  "dependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "7.1.4",
    "babel-plugin-lodash": "3.3.2",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-1": "6.5.0",
    "bootstrap": "4.0.0-alpha.3",
    "bullet-train-client": "0.0.52",
    "bullet-train-rules-engine": "0.0.7",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "0.1.10",
    "colors": "^1.3.0",
    "copy-webpack-plugin": "4.5.1",
    "cross-env": "^5.2.0",
    "css-loader": "0.23.1",
    "dotenv": "^6.1.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "flux-react-dispatcher": "1.2.5",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "2.0.0",
    "ionicons": "^4.0.0",
    "json-loader": "0.5.4",
    "lazyframe": "^1.1.6",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "node-fetch": "^2.1.2",
    "node-sass": "4.11.0",
    "polyfill-react-native": "^1.0.5",
    "postcss-loader": "2.1.2",
    "promise-polyfill": "^7.1.2",
    "rc-switch": "^1.6.0",
    "react": "^15.3.0",
    "react-addons-shallow-compare": "^15.3.0",
    "react-async-script": "^0.9.0",
    "react-dom": "^15.3.0",
    "react-hot-loader": "4.0.1",
    "react-maskedinput": "^4.0.1",
    "react-router": "2.6.0",
    "react-select": "^2.4.3",
    "react-tooltip": "^3.6.0",
    "sass-loader": "6.0.7",
    "slackbots": "^1.2.0",
    "ssg-node-express": "4.16.3",
    "ssg-util": "0.0.2",
    "style-loader": "0.13.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "4.4.1",
    "webpack-cli": "2.0.13",
    "webpack-dev-middleware": "3.1.0",
    "webpack-hot-middleware": "2.21.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chromedriver": "^2.33.2",
    "env2": "^2.2.0",
    "nightwatch": "^0.9.21",
    "node-slack-upload": "^1.2.1",
    "nodemon": "^1.17.5",
    "raw-loader": "^0.5.1",
    "react-markdown": "^4.0.3",
    "selenium-server": "^3.8.1"
  }
